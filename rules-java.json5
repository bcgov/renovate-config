{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "groupName": "maven all non-major dependencies",
      "groupSlug": "maven all-minor-patch",
      "matchManagers": [
        "maven"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch",
        "lockFileMaintenance"
      ],
      "minimumReleaseAge": "7 days"
    }
  ]
}
